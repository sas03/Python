graph = {"A":["B","E"],"B":["E"], "E":["A","B","C","D"]}
 
def kanten(g):
	kante = []
	for knoten in g: # knoten ist in der liste g
#		kante.append(knoten) # return ['A','B','E']
		for n in g[knoten]:
			kante.append((knoten,n)) # return SchlÃ¼ÃŸel(knoten) - Wert(n) Paar
	return kante
 
print(kanten(graph))
